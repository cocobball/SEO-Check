{  "manifest_version": 3,  "name": "H1 Checker",  "description": "Checks for the presence of an H1 tag on the current page",  "version": "1.0",  "action": {    "default_popup": "popup.html",    "default_icon": {      "16": "icon16.jpg",      "48": "icon48.jpg",      "128": "icon128.jpg"    }  },  "permissions": ["activeTab"],  "content_scripts": [    {      "matches": ["<all_urls>"],      "js": ["content.js"]    }  ],  "icons": {    "16": "icon16.jpg",    "48": "icon48.jpg",    "128": "icon128.jpg"  }}